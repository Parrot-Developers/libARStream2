#! /bin/sh

if [ -e Makefile ] ; then
	make clean
fi

# Generated files.
rm -f \
    INSTALL \
    Makefile \
    Makefile.in \
    aclocal.m4 \
    config.h \
    config.h.in \
    config.log \
    config.status \
    configure \
    libtool \
    libbeaver_dbg.la \
    libbeaver.la \
    stamp-h1

find .. -name '*~' -delete

find .. -name '.dirstamp' -delete

# Generated directories.
rm -rf \
    doxygen-doc \
    autom4te.cache \
    *DSYM* \
    config \
    m4

find .. -name '.deps' -exec rm -rf {} \; 2> /dev/null
